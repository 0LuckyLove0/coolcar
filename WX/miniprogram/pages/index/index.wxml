<map
  id="map"
  latitude="{{location.latitude}}"
  longitude="{{location.longitude}}"
  scale="{{scale}}"
  setting="{{setting}}"
  enable-overlooking="{{enableOverlooking}}"
  enable-3D="{{enable3D}}"
  markers="{{markers}}"
>
  <cover-view class="item-container">
    <cover-image src="/resources/account.png" bindtap='onMyTripsTap'></cover-image>
    <cover-image 
    src="/resources/my_location.png"
    bindtap="moMyLocationTap"
    ></cover-image>
  </cover-view>
  <cover-view class="btn-container bg-grey round" bindtap="onScanTap">
    <cover-view >扫码租车</cover-view>
  </cover-view>
</map>
<modal  id="licModal" title="身份验证" contents="需要身份验证才能租车哦"></modal>