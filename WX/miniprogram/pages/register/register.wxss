.lic-tmpl{
    width: 600rpx;
    height: 450rpx;
    position: relative;
    display: flex;
    justify-content: center;
    align-items: center;
}
.stretch{
    width: 100%;
    height: 100%;
}
.lic-tmpl-img{
    opacity: .3;
    position: relative;

}
.instruction{
    position: absolute;
    bottom: -34px;
}
.cuIcon-camerafill{
    font-size: 150rpx;
    position: absolute;
}
.container{
    padding: 203rpx;
}
.lic-form{
    width: 600rpx;
}
.cu-form-group .title {
    min-width: calc(4em + 30rpx);
}
.check {
    position: absolute;
    right: 0;
    top: 0;
    width: 100rpx;
    height: 100rpx;
}